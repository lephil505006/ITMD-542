extends layout

block content
  h1= title
  p= contact.firstName 
  p= contact.lastName 
  p= contact.email 
  p= contact.dateModified.ctime
    .contact-actions
      a(href='/contacts') Show all Todos 
      |
      a(href=`/contacts/${contact.id}/delete`) Delete Todo 
      |
      a(href=`/contacts/${contact.id}/edit`) Edit Todo 